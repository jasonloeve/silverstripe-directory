{
    "name": "toddhossack/silverstripe-directory",
    "description": "Directory module for SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jason Loeve",
            "email": "jason@weareonfire.co.nz"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "silverstripe/vendor-plugin": "^1",
        "silverstripe/framework": "^3",
        "silverstripe/recipe-cms": "^4.8.0",
        "zf1/zend-locale": "^1.12",
        "undefinedoffset/sortablegridfield": "^2.0",
        "symbiote/silverstripe-gridfieldextensions": "^3"
    },
    "extra": {
        "installer-name": "directory",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "autoload": {
        "psr-4": {
            "CS\\Directory\\": "./app/src"
        }
    }
}